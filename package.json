{
  "name": "11ty-uswds-template",
  "version": "0.0.2",
  "description": "A 11ty UWDS template.",
  "main": "index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "npm run assets:build && npx @11ty/eleventy",
    "build:prod": "ELEVENTY_ENV=production npm run build",
    "build:prod:url": "BASEURL=${BASEURL:-<default-url-here>} npm run build:prod",
    "build:comingsoon": "ELEVENTY_DIR=coming-soon npm run assets:build && ELEVENTY_DIR=coming-soon npx @11ty/eleventy",
    "build:comingsoon:prod": "ELEVENTY_ENV=production npm run build:comingsoon",
    "build:comingsoon:prod:url": "BASEURL=${BASEURL:-<default-url-here>} npm run build:comingsoon:prod",
    "assets:autoprefix": "postcss _site/assets/styles/*.css --map=false -r --use autoprefixer",
    "assets:build": "node ./config/buildAssets && npm run assets:autoprefix",
    "assets:clean": "rimraf _site/assets",
    "assets:refresh": "npm run assets:clean && npm run assets:build",
    "assets:watch": "chokidar 'js/**' 'styles/**' -c 'npm run assets:refresh' --polling",
    "clean": "rimraf _site",
    "dev": "npm run clean && npm-run-all -p dev:assets dev:serve",
    "dev:clean": "npm run clean && npm run dev",
    "dev:assets": "npm run assets:refresh && npm run assets:watch",
    "dev:debug": "DEBUG=* npx @11ty/eleventy --serve --watch",
    "dev:serve": "npx @11ty/eleventy --serve --watch",
    "pages": "npm run build",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx @11ty/eleventy --serve",
    "xml_sitemap_validate": "node _test/sitemap_test.js",
    "pa11y": "node _test/pa11y.js",
    "lint": "eslint $(find . -type f -not -path './node_modules/*' -not -path '.*.js' -name '*.js')",
    "links": "check-html-links _site/",
    "data": "npm run data:clear && npm run data:transform",
    "data:transform": "node _data/data-transformation/transform.js",
    "data:clear": "node _data/data-transformation/clear-exclusions.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "CC0-1.0",
  "devDependencies": {
    "@exodus/schemasafe": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "chokidar-cli": "^3.0.0",
    "csv-parser": "^3.0.0",
    "eleventy-plugin-svg-sprite": "^2.5.0",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^8.57.0",
    "eslint-config-xo": "^0.44.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.0",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@uswds/uswds": "3.11.0",
    "check-html-links": "^0.2.4",
    "flexsearch": "^0.7.43",
    "objects-to-csv": "^1.3.6",
    "pa11y": "^8.0.0",
    "tough-cookie": "^4.1.3",
    "xml2js": "^0.6.2"
  }
}
